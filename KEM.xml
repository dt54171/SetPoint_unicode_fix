  <!-- BatteryStatusInfoSet -->
  <!-- TEST-Set the ICONINDEX according to this table:
		IDI_BATTERY_LEVEL_LOW					0
		IDI_BATTERY_LEVEL_FAIR					1
		IDI_BATTERY_LEVEL_GOOD					2
		IDI_BATTERY_LEVEL_FULL					3
		IDI_BATTERY_LEVEL_CRITICAL				4

  	   -->
  <BatteryStatusInfoSet>
    <BatteryStatusInfo NumLevels="2">
      <BatteryStatus Status="1" StatusString="IDS_BATT_LEVEL_CRITICAL" IconIndex="4"/>
      <BatteryStatus Status="2" StatusString="IDS_BATT_LEVEL_GOOD" IconIndex="2"/>
